
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@inject IViewLocalizer L
<h2>@L["Currencies"]</h2>



<form method="get" class="mb-3">
    <input name="q" class="form-control" placeholder="@L["Search"]" value="@Context.Request.Query["q"]" />
</form>

<a asp-action="Create" class="btn btn-primary mb-3">@L["NewCurrency"]</a>




